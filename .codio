{
// Configure your Run and Preview buttons here.

// Run button configuration
  "commands": {
        "compile && run": {
          "id": "backend-guide",
          "cmd": "cd ./{{path}} && g++ {{filepath}} -o {{filename_no_ext}} && ./{{filename_no_ext}}"
        },
        "compile": {
          "id": "backend-guide",
          "cmd": "cd ./{{path}} && g++ {{filepath}} -o {{filename_no_ext}}"
        },
        "run": {
          "id": "backend-guide",
          "cmd": "cd ./{{path}} && ./{{filename_no_ext}}"
        }
  },

// Preview button configuration
  "preview": {
        "Project Index (static)": "https://{{domain}}/{{index}}",
        "Current File (static)": "https://{{domain}}/{{filepath}}",
        "Box URL": "https://{{domain3000}}/"
  }
}
